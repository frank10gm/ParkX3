		x:Class="ParkX3.MarchePage"
		>
	<ContentPage.Content>

	<ListView x:Name="MarcheView" ItemSelected="OnSelection">
    <ListView.ItemTemplate>
      <DataTemplate>
        <TextCell Text="{Binding marca}"
					/>
      </DataTemplate>
    </ListView.ItemTemplate>
  </ListView>
		
	</ContentPage.Content>
</ContentPage>
